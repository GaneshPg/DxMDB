@model DxMDB.Models.Producer

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Delete</title>
</head>
<body>
    <h3>Are you sure you want to delete @Model.Name?</h3>
    <div>

        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

            <div class="form-actions no-color">
                <input type="submit" value="Yes" class="btn btn-default" />
                <input type="button" value="Cancel" class="btn btn-default" id="cancel" />
            </div>
        }

    </div>
</body>
</html>

<script>
    $('#cancel').on('click', function () {
        $('#closeModal').click();
    });
</script>
